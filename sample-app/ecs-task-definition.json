        {
            "name": "nestjs-container",
            "image": "ghcr.io/middleware-labs/nest-apm:demo",
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ]
}